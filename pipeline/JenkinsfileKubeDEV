node {
        stage( 'Hello A' ) {
            echo 'Hello from branch ' + scm.branches[0].name 
            sh( 'env' )
            sh( 'set' )
        }

        stage( 'Checkout' ) {
            checkout scm
        }

        stage( 'Hello2' ) {
            sh( 'env' )
            sh( 'set' )
        }

        stage ( 'Try git' ) {
            sh( "git branch" )
            sh( "git status")
            echo 
        }
}